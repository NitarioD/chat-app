ame=\"header\">\n  <div  onClick={()=>setDisplay(false)} className='logo-container'>\n    <img src={Logo} alt='logo' className='logo'/>\n  </div>\n  <span className=\"header-user\">Hello {props.user}</span>\n  { (display)? <MenuBox />: <img src={Kebab} className='kebab' onClick={()=>setDisplay(true)}/> }\n  </div>);\n}\nexport default Header;","/data/data/com.termux/files/home/chat-app/react-app/src/components/message-box.jsx",["121"],[],"import { useNavigate } from 'react-router-dom';\n\nfunction MessageBox(props){\n  \n  const navigate= useNavigate();\n  \n  return(<div onClick={ ()=>{props.setReceiver(props.username);navigate('/conversations')} } className=\"message-group\">\n  <div className=\"img-box\"><img alt=\"user's photo\" src={props.img}/></div>\n  <div className=\"message-box\">\n    <h3>~ {props.username}</h3>\n    <p>{props.message}</p>\n  </div>\n  </div>);\n}\nexport default MessageBox;","/data/data/com.termux/files/home/chat-app/react-app/src/pages/chat-page.jsx",[],[],"/data/data/com.termux/files/home/chat-app/react-app/src/pages/settings-page.jsx",[],[],["122","123"],"/data/data/com.termux/files/home/chat-app/react-app/src/pages/access-page.jsx",[],[],"/data/data/com.termux/files/home/chat-app/react-app/src/components/menubox.jsx",[],[],"/data/data/com.termux/files/home/chat-app/react-app/src/pages/profile-page.jsx",[],[],"/data/data/com.termux/files/home/chat-app/react-app/src/components/profile-pic.jsx",["124"],[],"function ProfilePic(props){\n  return(<img className=\"profile-pic\" src={props.imgSrc}/>);\n}\nexport default ProfilePic;","/data/data/com.termux/files/home/chat-app/react-app/src/components/state-tag.jsx",[],[],"/data/data/com.termux/files/home/chat-app/react-app/src/pages/conversation-page.jsx",["125","126","127"],[],"import axios from 'axios';\nimport { useState, useEffect, useRef } from 'react';\nimport StateTag from '../components/state-tag';\nimport send from '../images/send.svg';\n\nfunction ConvoPage(props){\n  const receiver= props.receiver;\n  const [ message, setMessage ]= useState('');\n  const [ refresh, setRefresh ]= useState(false);\n  \n  const messagesEndRef = useRef(null);\n  \n  const [ convoList, setConvoList]= useState([]);\n  useEffect( ()=>{\n    const getConvos= async ()=>{\n      const { data }= await axios.get('/conversations/'+receiver);\n      setConvoList(data.conversations);\n      messagesEndRef.current.scrollIntoView({ behavior: \"smooth\" });\n    }\n    getConvos();\n  }, [refresh])\n  \n  useEffect(() => {\n  const timer = setInterval( async () => {\n    const { data }= await axios.get('/conversations/'+receiver);\n      setConvoList(data.conversations);\n      messagesEndRef.current.scrollIntoView({ behavior: \"smooth\" });\n  }, 3000);\n  return () => clearInterval(timer);\n}, []);\n  \n  function handleSend(){\n    if( message !== ''){\n      const sendConvo= async ()=>{\n      const { data }= await axios.post('/conversations/'+receiver, {\n          message: message\n      });\n      if (data === 'sent'){\n        setMessage('');\n        setRefresh(!refresh);\n      }\n    }\n    sendConvo();\n    }\n    \n  }\n  \n  return(<div className=\"convo\">\n    <StateTag\n    page= {receiver}\n    backTo= \"ChatPage\" />\n    {convoList.map( (eachConvo)=>(<div className=\"chat\" key= {eachConvo._id}>\n    <span className= {(eachConvo.sender === receiver)? 'common-chat friend-chat': 'common-chat user-chat'}>{eachConvo.message}</span>\n    </div>))}\n    <div className='space' ref={messagesEndRef}></div>\n    \n    <div className=\"compose\">\n      <input placeholder=\"write message\" onChange={(e)=>(setMessage(e.target.value))} value={message}/>\n      <img src= {send} onClick= {handleSend}/>\n    </div>\n  </div>);\n}\nexport default ConvoPage;","/data/data/com.termux/files/home/chat-app/react-app/src/pages/users-page.jsx",["128"],[],"import { useEffect, useState } from 'react';\nimport axios from 'axios';\nimport { useNavigate } from 'react-router-dom';\n\nimp